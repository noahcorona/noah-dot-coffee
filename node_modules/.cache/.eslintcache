na/Development/web/noah-dot-coffee/src/App.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/pages/Contact.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/pages/Error404.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/pages/Portfolio.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/pages/About.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/constants/navigation.js",[],[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/constants/projects.js",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/Cursor.tsx",["122"],[],"// cursor\nimport {useEffect, useState} from \"react\";\nimport classNames from \"classnames\";\nimport '../style/Cursor.css';\n\nexport const Cursor = () => {\n    const [position, setPosition] = useState({ x: 0, y: 0 });\n    const [clicked, setClicked] = useState(false);\n    const [linkHovered, setLinkHovered] = useState(false);\n    const [hidden, setHidden] = useState(false);\n\n    useEffect(() => {\n        addEventListeners();\n        handleLinkHoverEvents();\n        return () => removeEventListeners();\n    }, []);\n\n    const cursorClasses = classNames(\"cursor\", {\n        \"cursor--clicked\": clicked,\n        \"cursor--hidden\": hidden,\n        \"cursor--link-hovered\": linkHovered\n    });\n\n    const addEventListeners = () => {\n        document.addEventListener(\"mousemove\", onMouseMove);\n        document.addEventListener(\"mouseenter\", onMouseEnter);\n        document.addEventListener(\"mouseleave\", onMouseLeave);\n        document.addEventListener(\"mousedown\", onMouseDown);\n        document.addEventListener(\"mouseup\", onMouseUp);\n    };\n\n    const removeEventListeners = () => {\n        document.removeEventListener(\"mousemove\", onMouseMove);\n        document.removeEventListener(\"mouseenter\", onMouseEnter);\n        document.removeEventListener(\"mouseleave\", onMouseLeave);\n        document.removeEventListener(\"mousedown\", onMouseDown);\n        document.removeEventListener(\"mouseup\", onMouseUp);\n    };\n\n    const onMouseMove = (e: any) => {\n        setPosition({ x: e.clientX, y: e.clientY });\n    };\n\n    const onMouseDown = () => {\n        setClicked(true);\n    };\n\n    const onMouseUp = () => {\n        setClicked(false);\n    };\n\n    const onMouseLeave = () => {\n        setHidden(true);\n    };\n\n    const onMouseEnter = () => {\n        setHidden(false);\n    };\n\n    const handleLinkHoverEvents = () => {\n        document.querySelectorAll(\"a\").forEach((el) => {\n            el.addEventListener(\"mouseover\", () => setLinkHovered(true));\n            el.addEventListener(\"mouseout\", () => setLinkHovered(false));\n        });\n    };\n\n    return(\n        <div\n            className={ cursorClasses }\n            style={ { left: `${ position.x }px`, top: `${ position.y }px` } }\n        />);\n}\n","/Users/noahcorona/Development/web/noah-dot-coffee/src/components/portfolio/Project.tsx",[],[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/context/MouseContext.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/context/UseMousePosition.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/pages/Resume.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/NavBar/NavBar.tsx",["123"],[],"import '../../style/Navigation.css';\nimport navigation from \"../../constants/navigation\";\nimport NavItem from \"./NavItem\";\nimport SocialBar from \"./SocialBar\";\nimport {MouseContext} from \"../../context/MouseContext\";\nimport {useContext, useLayoutEffect, useRef} from \"react\";\n\ninterface IProps {\n    height: number,\n    setHeight: (height: number) => void,\n}\n\nconst NavBar = (props: IProps) => {\n    const { cursorChangeHandler } = useContext(MouseContext);\n    const ref = useRef<HTMLDivElement>(null);\n\n    useLayoutEffect(() => {\n        if (!ref.current || !ref.current.clientHeight) {\n            return;\n        }\n\n        props.setHeight(ref.current.clientHeight);\n    }, []);\n\n    return (\n        <div ref={ref} className=\"nav-bar\">\n            <div className=\"nav-content\">\n                    <a href=\"/\"\n                       className=\"nav-logo-text\"\n                       onMouseEnter={() => cursorChangeHandler(\"hovered\")}\n                       onMouseLeave={() => cursorChangeHandler(\"\")}\n                    >\n                        noah.coffee\n                    </a>\n                    <SocialBar />\n                    <nav className=\"nav-container\">\n                        <ul className=\"nav-menu\">\n                            {navigation.map((item) =>\n                                <NavItem key={item.title}\n                                         item={item}\n                                         navHeight={props.height} />\n                            )}\n                        </ul>\n                    </nav>\n            </div>\n        </div>\n    )\n}\n\nexport default NavBar;\n","/Users/noahcorona/Development/web/noah-dot-coffee/src/components/ResumeForm.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/ContactForm.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/Portfolio/Project.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/NavBar/NavItem.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/NavBar/SocialBar.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/style/inputStyles.js",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/Portfolio/TabContainer.tsx",[],[],"/Users/noahcorona/Development/web/noah-dot-coffee/src/components/Portfolio/Carousel.tsx",[],[],{"ruleId":"124","severity":1,"message":"125","line":16,"column":8,"nodeType":"126","endLine":16,"endColumn":10,"suggestions":"127"},{"ruleId":"124","severity":1,"message":"1